<p id="notice"><%= notice %></p>

<h1>Notificaciones</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Cuerpo</th>
      <th>Url</th>
      <th>Leido</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notificaciones.each do |notificacion| %>
      <tr>
        <td><%= notificacion.titulo %></td>
        <td><%= notificacion.cuerpo %></td>
        <td><%= notificacion.url %></td>
        <td><%= notificacion.leido %></td>
        <td><%= notificacion.user_id %></td>
        <td><%= link_to 'Show', notificacion %></td>
        <td><%= link_to 'Edit', edit_notificacion_path(notificacion) %></td>
        <td><%= link_to 'Destroy', notificacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Notificacion', new_notificacion_path %>
