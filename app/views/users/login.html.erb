<% provide(:title, "Login") %>

<%= form_tag '/authenticate' do |f| %>
  <div class="field">
    <%= label :email, "email" %>
    <%= email_field_tag :email %>
  </div>
  <div class="field">
    <%= label :password, "password" %>
    <%= password_field_tag :password %>
  </div>
  <%= submit_tag "Ingresar" %>
<% end %>